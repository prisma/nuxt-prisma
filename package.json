{
  "name": "@prisma/nuxt",
  "version": "0.1.0",
  "description": "Integrate Nuxt with Prisma - Compatible with Nuxt 4",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/prisma/nuxt-prisma.git"
  },
  "license": "MIT",
  "type": "module",
  "exports": {
    ".": {
      "types": "./dist/types.d.mts",
      "import": "./dist/module.mjs"
    }
  },
  "main": "./dist/module.mjs",
  "files": [
    "dist"
  ],
  "scripts": {
    "prepack": "nuxt-module-build build",
    "dev": "nuxi dev playground",
    "dev:build": "nuxi build playground",
    "dev:prepare": "nuxt-module-build build --stub && nuxt-module-build prepare && nuxi prepare playground",
    "docs": "cd docs && npm run dev",
    "release": "npm run lint && npm run prepack && changelogen --release && npm publish --access public && git push --follow-tags",
    "lint": "eslint .",
    "test": "vitest run",
    "test:watch": "vitest watch",
    "link": "npm link",
    "clean:playground": "rm -r ./playground/prisma ./playground/lib ./playground/node_modules",
    "prepare": "husky install",
    "commit": "npx git-cz",
    "commitlint": "commitlint --edit"
  },
  "dependencies": {
    "@nuxt/devtools-kit": "^2.6.5",
    "@nuxt/kit": "^4.1.3",
    "@prisma/client": "^6.17.1",
    "consola": "^3.3.3",
    "defu": "^6.1.4",
    "pathe": "^1.1.2",
    "tinyexec": "^0.3.2",
    "tinyrainbow": "^1.2.0"
  },
  "devDependencies": {
    "@commitlint/cli": "^19.5.0",
    "@commitlint/config-conventional": "^19.5.0",
    "@commitlint/cz-commitlint": "^19.5.0",
    "@nuxt/devtools": "^2.6.5",
    "@nuxt/eslint-config": "^0.2.0",
    "@nuxt/module-builder": "^1.0.2",
    "@nuxt/schema": "^4.1.3",
    "@nuxt/test-utils": "^3.19.2",
    "@types/node": "^22.10.0",
    "@types/prompts": "^2.4.9",
    "changelogen": "^0.5.5",
    "commitizen": "^4.3.1",
    "cz-customizable": "^5.5.0",
    "eslint": "^8.56.0",
    "husky": "^9.1.7",
    "inquirer": "^9.3.7",
    "nuxt": "^4.1.3",
    "prettier": "^3.4.1",
    "prisma": "^6.17.1",
    "semantic-release": "^24.2.0",
    "vitest": "^3.2.0"
  },
  "config": {
    "commitizen": {
      "path": "node_modules/cz-customizable"
    },
    "cz-customizable": {
      "config": "./.cz-config.cjs"
    }
  }
}
